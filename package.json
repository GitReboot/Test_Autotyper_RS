{
  "name": "electron-autotyper",
  "version": "0.0.0",
  "private": true,
  "main": "electron/main.js",
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron ./electron/main.js",
    "electron:dev": "concurrently -k \"vite\" \"npm run electron\"",
    "electron:build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "files": [
      "dist/**/*",
      "electron/**/*",
      "src/preload.cjs",
      "src/main.js",
      "!index.html"
    ],
    "extraResources": [
      "public"
    ]
  },
  "dependencies": {
    "@nut-tree-fork/nut-js": "^4.2.6",
    "active-win": "^8.2.1",
    "axios": "^1.10.0",
    "canvas": "^3.1.2",
    "chokidar": "^4.0.3",
    "electron-store": "^10.1.0",
    "fs": "^0.0.1-security",
    "image-size": "^2.0.2",
    "lucide-vue-next": "^0.525.0",
    "node-process-windows": "^0.0.2",
    "screenshot-desktop": "^1.15.1",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "vite": "^7.0.4"
  }
}
